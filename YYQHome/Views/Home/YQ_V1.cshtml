@{
    Layout = null;
}

<!DOCTYPE html>
<html>

<head>
    <meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
    <meta name="Robots" content="All" />
    <meta name="Author" content="AQiu个人主页" />
    <meta name="Copyright" content="AQiu个人主页,YYQ个人主页,YYQ,AQiu" />
    <meta name="keywords" content="AQiu个人主页,YYQ个人主页,YYQ,AQiu" />
    <meta name="description" content="AQiu个人主页,YYQ个人主页,YYQ,AQiu" /> 
    <title>AQiu个人主页</title>
     <style type="text/css">
        /*初始化*/
        *{margin:0 auto;padding:0;}
        body{ padding:0;margin:0 auto; font-family:Microsoft YaHei,SimSun;position:relative; background:none; background:#fff;}
        p,ul,ul li,form{padding:0; margin:0; list-style:none;}
        a{ text-decoration:none;}
        img{border:0;}
        /*头部*/
        .HeadBox{height:100px;background:black;position:relative;}
        .HeadLogo{position:absolute;left:50px;top:30px;  font-weight:bolder;color:white;margin:0 0;-moz-transition: all 0.8s ease-in-out; -webkit-transition: all 0.8s ease-in-out; -o-transition: all 0.8s ease-in-out;-ms-transition: all 0.8s ease-in-out;transition: all 0.8s ease-in-out;}
        .HeadLogo:hover{-moz-transform: rotate(360deg);-webkit-transform: rotate(360deg); -o-transform: rotate(360deg);-ms-transform: rotate(360deg);transform: rotate(360deg);}
        /*主要模块部分*/
        .MainBox{margin-top:1px;}
        .MainTop{height:30px;width:1000px;}
        .AllContent{margin-left:50px;min-height:500px;}
        .ContentDetail{margin-left:20px;}
        .ItemContent{float:left;margin-left:10px;min-height:200px;}
        .ContentDetail ul li{display:block;border-left:1px solid black;padding-top:2px; font-size:13px;}
        .ContentTitle{border-bottom:1px solid black;}
        /*尾部*/
        .FootBox{height:400px;background:url("/Content/YQ/Images/bg.jpg") no-repeat 0 80px scroll;margin-top:1px; }
        /*其他公用*/
        .ClearFloat{clear:both;height:10px;border-bottom:1px dotted black; }
        /*返回顶部*/
        .TopBox{display:none;height:50px;width:25px;background:url("/Content/YQ/Images/top.png") no-repeat 0 0 scroll; position:fixed; _position: absolute;_top: expression(documentElement.scrollTop + documentElement.clientHeight-this.offsetHeight);right:20px; bottom:20px;}
        .TopBoxHover{background:url("/Content/YQ/Images/top.png") no-repeat -27px 0 scroll;}
        /*人形时间*/
        .TimeBox{position:absolute;right:20px;top:75px;}
    </style>
</head>
<body>
    <div class="HeadBox">
        <h1 class="HeadLogo">AQiu</h1>
    </div>
    <div class="MainBox">
        <div class="MainTop"></div>
        
    </div>
    <!--评论模块-->
    <div>
        <!-- 多说评论框 start -->
        <div class="ds-thread" id="dsBox" data-thread-key="Home" data-title="" data-url="http://qiu.moepet.info/Home/YQ/"></div>
        <!-- 多说评论框 end -->
    </div>
      <!--评论模块-->
    <div class="FootBox"></div>
    <div id="TopBox" class="TopBox" style="z-index:99;"></div>
    <div class="TimeBox"><script charset="Shift_JIS" src="http://chabudai.sakura.ne.jp/blogparts/honehoneclock/honehone_clock_tr.js"></script></div>
    <script src="/JS/jquery.1.9.1.min.js" type="text/javascript"></script>
    <script src="/JS/jquery.easing.min.js" type="text/javascript"></script>
    <!-- 多说公共JS代码-->
    <script type="text/javascript">
     
        var duoshuoQuery = { short_name: "sflyq" };
        (function () {
            //绑定数据
            $("#dsBox").attr({ "data-thread-key": location.pathname, "data-url": location.href, "data-title": $("title") ? $("title").val() : "" });
            //绑定数据
            var ds = document.createElement('script');
            ds.type = 'text/javascript'; ds.async = true;
            ds.src = (document.location.protocol == 'https:' ? 'https:' : 'http:') + '//static.duoshuo.com/embed.unstable.js';
            ds.charset = 'UTF-8';
            (document.getElementsByTagName('head')[0]
                || document.getElementsByTagName('body')[0]).appendChild(ds);

            //隐藏留言板块设置
            setTimeout(function () {
                $(".ds-powered-by a").hide().css({ "dispaly": "none", "height": "0px", "width": "0px;" });
                $("#ds-sync-checkbox").click().prop("checked", false);
            }, 2000);
            //隐藏留言板块设置
        })();
    </script>
    <!-- 多说公共JS代码-->
    <script type="text/ecmascript">
        /// <reference path="/JS/jquery.easing.min.js" />
        $(window).scroll(function () {
            if ($(window).scrollTop() > 10) {
                $("#TopBox").fadeIn(100);
            } else {
                $("#TopBox").fadeOut(100);
            }
        });
        $("#TopBox").hover(function () {
            $topBoxJq = $(this);
            $topBoxJq.addClass("TopBoxHover");
        }, function () {
            $topBoxJq = $(this);
            $topBoxJq.removeClass("TopBoxHover");
        }).click(function () {
            $(window).scrollTop(0);
        });

    </script>
    <script type="text/javascript" src="http://tajs.qq.com/stats?sId=30702953" charset="UTF-8"></script>
</body>
</html>
